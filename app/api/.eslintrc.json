{
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "api": "readonly",
    "common": "readonly",
    "crud": "readonly",
    "db": "readonly",
    "domain": "readonly",
    "excel": "readonly",
    "setting": "readonly",
    "responseType": "readonly",
    "lib": "readonly"
  },
  "rules": {
    "strict": ["error", "never"],
    "id-denylist": [
      2,
      "api",
      "common",
      "console",
      "crud",
      "db",
      "domain",
      "excel",
      "setting",
      "responseType",
      "lib"
    ]
  }
}
